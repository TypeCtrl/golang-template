{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "Basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint -f codeframe src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "12.12.22",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.22.0",
    "eslint-config-xo-typescript": "0.21.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "semantic-release": "15.13.30",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
