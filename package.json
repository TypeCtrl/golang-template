{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "source": "src/index.ts",
  "main": "./golang-template.js",
  "module": "./golang-template.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/lodash.get": "4.4.4",
    "@types/node": "10.12.19",
    "jest": "23.6.0",
    "microbundle": "0.9.0",
    "prettier": "1.16.3",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.14.0",
    "typescript": "3.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
