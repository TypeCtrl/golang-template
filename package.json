{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "Basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "module": "./module/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/lodash.get": "4.4.6",
    "@types/node": "12.0.4",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.14.0",
    "eslint-plugin-import": "2.17.3",
    "jest": "24.8.0",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
