{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "Basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/module/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint -f codeframe src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc && tsc -p tsconfig.module.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash.get": "4.4.6",
    "@types/node": "12.6.6",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "del-cli": "2.0.0",
    "eslint": "6.0.1",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.15.0",
    "eslint-plugin-import": "2.18.0",
    "jest": "24.8.0",
    "semantic-release": "15.13.18",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
